<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <solid android:color="#FFFFFF"/> <!-- Assuming a white background -->
    <stroke android:width="2dp" android:color="#FF000000"/> <!-- Black stroke -->
    <padding android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp"/>
    <size android:width="120dp"
        android:height="120dp"/>
    <corners android:radius="60dp"/> <!-- Half of the width and height to make it circular -->
</shape>
